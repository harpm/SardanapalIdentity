		<PackageReference Include="Sardanapal.Identity.OTP" Version="0.1.6" />
		<PackageReference Include="Sardanapal.Identity.OTP.Model" Version="0.1.3" />
		<PackageReference Include="Sardanapal.Identity.Share" Version="0.1.0" />
